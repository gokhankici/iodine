qualif Bla(x:int, y:int) : (x = y)

bind 1 x    : {v: int | true }
bind 2 y    : {v: int | true }
bind 4 arg2 : {v: int | true }
bind 5 xo   : {v: int | v=Map_select f x }
bind 6 yo   : {v: int | v=Map_select f y }
bind 7 f    : {v : Map_t Int Int | true}


constraint:
  env [1;2]
  lhs {v : int | [x=y]}
  rhs {v : int | $inv0[arg1 := x][arg2 := y]}
  id 1 tag []

constraint:
  env [1;2;5;6;7]
  lhs {v : int | $inv0[arg1 := x][arg2 := y]}
  rhs {v : int | $inv0[arg1 := xo][arg2 := yo]}
  id 2 tag []

constraint:
  env [1;2]
  lhs {v : int | $inv0[arg1 := x][arg2 := y]}
  rhs {v : int | x=y}
  id 3 tag []

wf:
  env [4]
  reft {arg1 : int | $inv0 }
